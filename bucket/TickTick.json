    },
    "architecture": {
        "64bit": {
            "url": "https://cdn.dida365.cn/download/win64/dida_win_setup_release_x64_6420.exe",
            "hash": "9b69886a39a7bcaaf534a164f9ff37bc9f95c05496d83f4d0e8fd50129f56498"
        },
        "32bit": {
            "url": "https://cdn.dida365.cn/download/win/dida_win_setup_release_x86_6420.exe",
            "hash": "884bb5e53306e5f5d3f316d2f57ee9e849092f12d00950858ae577858a7ab7a1"
        }
    },
    "innosetup": true,
    "shortcuts": [
        [
            "TickTick.exe",
            "滴答清单"
        ]
    ],
    "pre_install": [
        "Import-Module $(Join-Path $(Find-BucketDirectory -Root -Name air) scripts/AirUtils.psm1)",
        "RedirectDirectory \"$env:APPDATA\\Tick_Tick\" \"$persist_dir\\APPDATA\\Tick_Tick\"",
        "Remove-Module -Name AirUtils"
    ],
    "uninstaller": {
        "script": [
            "if ($cmd -eq 'uninstall') {",
            "    Import-Module $(Join-Path $(Find-BucketDirectory -Root -Name air) scripts/AirUtils.psm1)",
            "    RemoveJunction \"$env:APPDATA\\Tick_Tick\"",
            "    Remove-Module -Name AirUtils",
            "}"
        ]
    },
    "checkver": {
        "url": "https://pull.dida365.com/windows/release_note.json",
        "jsonpath": "$.version"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn.dida365.cn/download/win64/dida_win_setup_release_x64_$cleanVersion.exe"
            },
            "32bit": {
                "url": "https://cdn.dida365.cn/download/win/dida_win_setup_release_x86_$cleanVersion.exe"
            }
        }
    }
}
